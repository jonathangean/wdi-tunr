<% @songs.each do |song| %>
   <div class='four columns'>
      <div class='two columns'>
        <h2><%= song[:title] %></h2>
     </div>
      <img src="<%= song[:artwork_url]%>">
      <form action="/songs/save" method="post">
        <%= token_tag %>
        <input type="hidden" name='track_id' value='<%= song[:itunes_song_id] %>'>
        <button type='submit'>Save</button>
      </form>
    </div>


<% end %>
